<UserControl x:Class="G3D.Diplôme.Importer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:controls="clr-namespace:System.Windows.Controls;assembly=DotNetProjects.Input.Toolkit"
             xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             xmlns:local="clr-namespace:G3D"
             xmlns:domain="clr-namespace:G3D.Domain"
             mc:Ignorable="d" 
             TextElement.FontWeight="Regular"
             TextElement.FontSize="14"
             FontFamily="{materialDesign:MaterialDesignFont}"
             d:DesignHeight="872" d:DesignWidth="1634">

    <UserControl.Resources>
        <Style x:Key="MaterialDesignChoiceChipOutlineRadioButton" TargetType="{x:Type RadioButton}">
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="Height" Value="34"/>
            <Setter Property="Margin" Value="0,0,20,0"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RadioButton}">
                        <Grid ClipToBounds="True">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0" Background="{TemplateBinding Background}" CornerRadius="5" Padding="42,0"/>
                            <Border x:Name="MouseOverBorder" Background="{TemplateBinding Foreground, Converter={StaticResource BrushRoundConverter}}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="5" Opacity="0"/>
                            <Border x:Name="SelectedBackgroundBorder" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="5" Opacity="0"/>
                            <ContentControl x:Name="Content" Foreground="{DynamicResource PrimaryHueDarkBrush}" ContentStringFormat="{TemplateBinding ContentTemplateSelector}" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" HorizontalAlignment="Center" IsTabStop="False" Margin="12 0 15 0" VerticalAlignment="Center"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Opacity" Value=".56"/>
                            </Trigger>

                            <Trigger Property="IsChecked" Value="False">
                                <Setter Property="Foreground" TargetName="Content" Value="#5f6368"/>
                                <Setter Property="Foreground" Value="#5f6368"/>
                                <Setter Property="BorderBrush" Value="{DynamicResource MaterialDesignDivider}"/>
                                <Setter Property="Background" Value="White"/>

                            </Trigger>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter Property="Foreground" Value="{StaticResource PrimaryHueMidBrush}"/>
                                <Setter Property="FontWeight" Value="Medium"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" TargetName="MouseOverBorder" Value=".03"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="RadioButtonStyle1" BasedOn="{StaticResource MaterialDesignChoiceChipOutlineRadioButton}" TargetType="{x:Type RadioButton}">
            <Setter Property="Background" Value="#e9f4fe"/>
            <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}"/>
        </Style>
    </UserControl.Resources>

    <Border Background="#f3f4fb" CornerRadius="5">
        <materialDesign:DialogHost DialogClosingCallback="{Binding DialogClosingHandler}" Name="MainDialog" CloseOnClickAway="True" OverlayBackground="#f3f4fb" DialogBackground="Transparent">
            <materialDesign:DialogHost.DialogContent>
                <Grid>
                    <Border Background="White" Padding="30" x:Name="ImportForm" CornerRadius="5" Visibility="Collapsed">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Foreground="#57606f" VerticalAlignment="Center" FontWeight="Medium" FontSize="13">Importer un document excel</TextBlock>
                            <Button Grid.Column="1" Visibility="Visible" x:Name="DialogClose"
                                Command="{x:Static materialDesign:DialogHost.CloseDialogCommand}"
  Style="{StaticResource MaterialDesignFlatLightButton}"
  ToolTip="Supprimer des duplicatas"
                                Height="30"
                                Foreground="#2c3e50"
                                Background="Transparent"
                                Padding="0"
                                materialDesign:ButtonAssist.CornerRadius="5"
                            Width="30">
                                <materialDesign:PackIcon
    Kind="Close"
    Height="18"
    Width="18" />

                            </Button>
                            <ProgressBar Grid.Column="1" x:Name="DialogLoading" Visibility="Hidden" Style="{StaticResource MaterialDesignCircularProgressBar}" Value="0" IsIndeterminate="True" />
                            <Border Margin="0,25" Grid.ColumnSpan="2" Grid.Row="1" BorderThickness="2" CornerRadius="5" Padding="80,40" Background="White" AllowDrop="True" Cursor="Hand" Name="ImportBorder" 
                                MouseEnter="ImportBorder_MouseEnter" MouseLeave="ImportBorder_MouseLeave" MouseLeftButtonDown="ImportBorder_MouseLeftButtonDown"
                                DragEnter="ImportBorder_MouseEnter" DragLeave="ImportBorder_MouseLeave"
                                Drop="ImportBorder_Drop"
                                >
                                <Border.Resources>
                                    <SolidColorBrush x:Key="ImportBorderBrush" x:Name="ImportBorderBrush" Color="#d1d8e0" />
                                </Border.Resources>
                                <Border.BorderBrush>
                                    <DrawingBrush Viewport="0,0,16,16" ViewportUnits="Absolute" TileMode="Tile">
                                        <DrawingBrush.Drawing>
                                            <DrawingGroup>
                                                <GeometryDrawing Brush="{DynamicResource ImportBorderBrush}">
                                                    <GeometryDrawing.Geometry>
                                                        <GeometryGroup>
                                                            <RectangleGeometry Rect="0,0,50,50" />
                                                            <RectangleGeometry Rect="50,50,50,50" />
                                                        </GeometryGroup>
                                                    </GeometryDrawing.Geometry>
                                                </GeometryDrawing>
                                            </DrawingGroup>
                                        </DrawingBrush.Drawing>
                                    </DrawingBrush>
                                </Border.BorderBrush>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height='*'/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="TrayArrowUp" Foreground="{DynamicResource ImportBorderBrush}" Width="50" Height="50" HorizontalAlignment="Center" Margin="0,0,0,25"/>
                                        <TextBlock Foreground="#57606f" TextAlignment="Center" FontSize="13">Sélectionnez un fichier excel pour importer</TextBlock>
                                        <TextBlock Foreground="#a4b0be" TextAlignment="Center" Margin="0,10,0,0" FontSize="12">Ou glissez et déposez-le ici</TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <ItemsControl Name="FileDropped" Grid.Row="2" Grid.ColumnSpan="2">
                                <ItemsControl.Resources>
                                    <DataTemplate DataType="{x:Type domain:Imports}">
                                        <Border Margin="0,0,0,25" Background="#f1f2f6" CornerRadius="5" Padding="15,10">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                                <materialDesign:PackIcon Kind="FileExcelOutline" Width="35" Height="35" Foreground="#778ca3" Margin="0,0,15,0" />
                                                <Grid Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <WrapPanel VerticalAlignment="Center">
                                                        <TextBlock FontWeight="Medium" Margin="0,0,5,0" Foreground="{StaticResource PrimaryHueMidBrush}" FontSize="11">(<Run Text="{Binding Count}"></Run>)</TextBlock>
                                                        <TextBlock Foreground="#57606f" FontSize="11" Text="{Binding Title}"></TextBlock>
                                                    </WrapPanel>
                                                    <WrapPanel Grid.Row="1" VerticalAlignment="Center">
                                                        <Border VerticalAlignment="Center" Background="#20bf6b" CornerRadius="5" Padding="8,1" Margin="0,0,5,0">
                                                            <TextBlock FontSize="10" Foreground="White" Text="{Binding Extrnsion}"></TextBlock>
                                                        </Border>
                                                        <TextBlock VerticalAlignment="Center" FontSize="10" Foreground="#57606f" Text="{Binding Size}"></TextBlock>
                                                    </WrapPanel>
                                                </Grid>
                                                <Button Grid.Column="2" VerticalAlignment="Center" Click="Button_Click"
  Style="{StaticResource MaterialDesignFlatLightButton}"
  ToolTip="Supprimer des duplicatas"
                                Height="30"
                                Foreground="#eb3b5a"
                                Background="Transparent"
                                Padding="0"
                                materialDesign:ButtonAssist.CornerRadius="5"
                            Width="30">
                                                    <materialDesign:PackIcon
    Kind="Close"
    Height="16"
    Width="16" />

                                                </Button>
                                            </Grid>
                                        </Border>
                                    </DataTemplate>
                                </ItemsControl.Resources>
                            </ItemsControl>
                            <Button  Name="ImportButton" IsEnabled="False" Click="ImportButton_Click"
                                       Grid.Row="3" Grid.ColumnSpan="2"
  Style="{StaticResource MaterialDesignFlatMidBgButton}"
  ToolTip="Supprimer des duplicatas"
                                Height="32"
                            Width="Auto"
                                Foreground="White"
                                Padding="25,0,25,3"
                                    BorderThickness="0"
                                materialDesign:ButtonAssist.CornerRadius="5"
                            >
                                <TextBlock FontSize="12">Importer</TextBlock>
                            </Button>
                        </Grid>
                    </Border>
                    <Border HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="AMStagiaire" Visibility="Collapsed" Background="White" CornerRadius="5" Padding="40,30">
                        <Grid Margin="0">
                            <Grid.Resources>
                                <Style TargetType="Grid">
                                    <Setter Property="Margin" Value="0,0,0,22"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid Margin="0,0,0,30">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Foreground="#57606f" FontWeight="Medium" FontSize="13" VerticalAlignment="Center" Name="AMSTitle"></TextBlock>
                                <Button Grid.Column="1" Visibility="Visible" Command="{x:Static materialDesign:DialogHost.CloseDialogCommand}"
  Style="{StaticResource MaterialDesignFlatLightButton}"
  ToolTip="Supprimer des duplicatas"
                                Height="30"
                                Foreground="#2c3e50"
                                Background="Transparent"
                                Padding="0"
                                materialDesign:ButtonAssist.CornerRadius="5"
                            Width="30">
                                    <materialDesign:PackIcon
    Kind="Close"
    Height="18"
    Width="18" />

                                </Button>
                            </Grid>
                            <StackPanel Grid.Row="1" Margin="0,0,0,10">
                                <StackPanel.Resources>
                                    <Style TargetType="materialDesign:PackIcon" x:Key="WarningIcon">
                                        <Setter Property="Foreground" Value="#d9254e"/>
                                        <Setter Property="Width" Value="13"/>
                                        <Setter Property="Height" Value="13"/>
                                        <Setter Property="Margin" Value="0,0,7,0"/>
                                        <Setter Property="Kind" Value="WarningCircle"/>
                                    </Style>
                                    <Style TargetType="TextBlock" x:Key="WarningText">
                                        <Setter Property="FontSize" Value="12"/>
                                        <Setter Property="Foreground" Value="#d9254e"/>
                                        <Setter Property="TextWrapping" Value="Wrap"/>
                                    </Style>
                                    <Style TargetType="WrapPanel" x:Key="WrapMargin">
                                        <Setter Property="Margin" Value="0,5,0,0"/>
                                    </Style>
                                </StackPanel.Resources>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="22"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel>
                                        <controls:AutoCompleteBox x:Name="AMSCef" Style="{DynamicResource AutoCompleteBoxStyle}" SelectionChanged="AMSCef_SelectionChanged" MinWidth="170" IsEnabled="True" IsTextCompletionEnabled="True" FilterMode="Contains" Padding="12,11" materialDesign:HintAssist.Hint="Cef" VerticalAlignment="Center"/>
                                        <WrapPanel Visibility="Collapsed" Name="AMSCefWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSCefWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2">
                                        <TextBox x:Name="AMSCin"
                    Style="{StaticResource MaterialDesignOutlinedTextBox}" 
                                 MinWidth="170"
                    TextWrapping="Wrap" 
                    Padding="12,11"
                    FontSize="12"
                                                 materialDesign:HintAssist.FloatingScale=".9"
                    Foreground="#57606f"
                    BorderBrush="#a4b0be"
                    materialDesign:HintAssist.Foreground="#a4b0be"
                    materialDesign:HintAssist.Background="White"
                    materialDesign:HintAssist.Hint="Cin"/>
                                        <WrapPanel Visibility="Collapsed" Name="AMSCinWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSCinWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <StackPanel>
                                        <TextBox x:Name="AMSNom"
                    Style="{StaticResource MaterialDesignOutlinedTextBox}" 
                    TextWrapping="Wrap" 
                    Padding="12,11"
                                                 materialDesign:HintAssist.FloatingScale=".9"
                    FontSize="12"
                    Foreground="#57606f"
                    BorderBrush="#a4b0be"
                    materialDesign:HintAssist.Foreground="#a4b0be"
                    materialDesign:HintAssist.Background="White"
                    materialDesign:HintAssist.Hint="Nom et prénom"/>
                                        <WrapPanel Visibility="Collapsed" Name="AMSNomWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSNomWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <StackPanel>
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSÉtablissement" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Établissement" SelectionChanged="AMSÉtablissement_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSÉtablissementWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSÉtablissementWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="22"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel>
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSAnnéeF" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Année de formation" SelectionChanged="AMSAnnéeF_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSAnnéeFWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSAnnéeFWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2">
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSNiveau" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Niveau" SelectionChanged="AMSNiveau_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSNiveauWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSNiveauWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="22"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel>
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSTypeS" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Type de stagiaire" SelectionChanged="AMSTypeS_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSTypeSWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSTypeSWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2">
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSTypeF" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Type de formation" SelectionChanged="AMSTypeF_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSTypeFWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSTypeFWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <StackPanel>
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSFilière" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Filière" SelectionChanged="AMSFilière_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSFilièreWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSFilièreWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="20"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel>
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSAnnéeE" Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Année d'étude" SelectionChanged="AMSAnnéeE_SelectionChanged">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSAnnéeEWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSAnnéeEWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2">
                                        <ComboBox materialDesign:HintAssist.FloatingScale=".9" Name="AMSGroupe"  Foreground="#57606f" BorderThickness="1" BorderBrush="#a4b0be" Padding="12,11" Style="{StaticResource MaterialDesignOutlinedComboBox}" materialDesign:HintAssist.Foreground="#a4b0be" FontSize="12"
                              materialDesign:HintAssist.Hint="Groupe">
                                            <ComboBox.Resources>
                                                <SolidColorBrush x:Key="MaterialDesignPaper" Color="White" />
                                            </ComboBox.Resources>
                                        </ComboBox>
                                        <WrapPanel Visibility="Collapsed" Name="AMSGroupeWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSGroupeWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="20"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel>

                                        <TextBox
                    Style="{StaticResource MaterialDesignOutlinedTextBox}" 
                    TextWrapping="Wrap" 
                                            materialDesign:HintAssist.FloatingScale=".9"
                    Padding="12,11" Name="AMSClassement"
                    FontSize="12"
                    Foreground="#57606f"
                    BorderBrush="#a4b0be"
                    materialDesign:HintAssist.Foreground="#a4b0be"
                    materialDesign:HintAssist.Background="White"
                    materialDesign:HintAssist.Hint="Classement"/>
                                        <WrapPanel Visibility="Collapsed" Name="AMSClassementWarning" Style="{StaticResource WrapMargin}">
                                            <materialDesign:PackIcon Style="{StaticResource WarningIcon}"/>
                                            <TextBlock Name="AMSClassementWarningText" Style="{StaticResource WarningText}"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <Border Grid.Column="2" Background="#f3f4f6" CornerRadius="5" Padding="20,0">
                                        <Grid VerticalAlignment="Center" Margin="0,-2,0,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="12" Foreground="#5f6368">Admis :</TextBlock>
                                            <WrapPanel Grid.Column="1" HorizontalAlignment="Right" Name="AMSAdmis" VerticalAlignment="Top">
                                                <RadioButton HorizontalAlignment="Center" Margin="0,0,25,0" Grid.Column="1" Style="{StaticResource MaterialDesignRadioButton}" Tag="Admis" Name="AMSAdmisOui" Content="Oui" IsChecked="True" FontSize="12" Foreground="#5f6368" Background="#2e86de"/>
                                                <RadioButton HorizontalAlignment="Center" Grid.Column="2" Style="{StaticResource MaterialDesignRadioButton}" Tag="Admis" Name="AMSAdmisNon" Content="Non" FontSize="12" Foreground="#5f6368" Background="#2e86de"/>
                                            </WrapPanel>
                                        </Grid>
                                    </Border>
                                </Grid>

                            </StackPanel>
                            <WrapPanel HorizontalAlignment="Right" Grid.Row="2">
                                <Button Style="{StaticResource MaterialDesignOutlinedLightButton}"
                        materialDesign:ButtonAssist.CornerRadius="5"
                        ToolTip="Ajouter un stagiaire manuellement"
                        Foreground="#57606f"
                        FontSize="12"
                        BorderThickness="0"
                        Command="{x:Static materialDesign:DialogHost.CloseDialogCommand}"
                        Padding="20,0" 
                        Margin="0,0,12,0"
                        Content="Annuler">
                                </Button>
                                <Button Style="{StaticResource MaterialDesignFlatMidBgButton}"
                        ToolTip="Supprimer des stagiaire" 
                        FontSize="12"
                        Background="#2e86de"
                        BorderThickness="0"
                        Padding="20,0,20,1"
                        Height="34"
                        Foreground="White"
                        materialDesign:ButtonAssist.CornerRadius="5" 
                        Name="SAButton"
                        Visibility="Visible"
                                        Click="SAButton_Click"
                        Content="Ajouter">
                                </Button>
                                <Button Style="{StaticResource MaterialDesignFlatMidBgButton}"
                        ToolTip="Supprimer des stagiaire" 
                        FontSize="12"
                        Background="#10ac84"
                        BorderThickness="0"
                        Padding="20,0,20,1"
                        Height="34"
                        Foreground="White"
                        materialDesign:ButtonAssist.CornerRadius="5" 
                                        Visibility="Collapsed"
                                        Click="SMButton_Click"
                        Name="SMButton"
                        Content="Modifier">
                                </Button>
                            </WrapPanel>
                        </Grid>
                    </Border>
                    <Border Background="White" Name="DeleteWarningDialog" CornerRadius="5" Visibility="Collapsed" MaxWidth="420">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Border Background="#eb3b5a" Padding="15,12,15,14" CornerRadius="5,5,0,0">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
